{
    "name": "genie",
    "displayName": "Genie",
    "description": "Language support for the Genie Client",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.43.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "genie",
            "aliases": ["Genie", "genie"],
            "extensions": [".cmd",".inc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "genie",
            "scopeName": "source.genie",
            "path": "./syntaxes/genie.tmLanguage.json"
        }]
    }
}